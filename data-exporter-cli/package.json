{
  "name": "ttt-data-exporter",
  "version": "1.0.0",
  "private": true,
  "description": "CLI to export games to JSON/CSV by date range and filters",
  "type": "module",
  "bin": {
    "ttt-export": "dist/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --target node20 --dts --out-dir dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "test": "vitest run --coverage"
  },
  "engines": { "node": ">=20.0.0" },
  "dependencies": {
    "commander": "^12.1.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.10.0",
    "@types/node": "^20.12.12",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.3",
    "tsup": "^8.2.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vitest": "^2.1.1"
  }
}

